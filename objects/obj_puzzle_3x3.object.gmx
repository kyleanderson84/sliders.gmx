            <string>///Input control
var sensitivity = 10;
if(mouse_check_button_pressed(mb_left)){
    start_x = mouse_x;
    start_y = mouse_y;
    puzzle_x = mouse_x div sprite_w;
    puzzle_y = mouse_y div sprite_w;
} else if(false and mouse_check_button(mb_left)){
    dif_x = mouse_x - start_x;
    dif_y = mouse_y - start_y;
    if( abs(dif_x) &gt; abs(dif_y)){
        for(var i = 0; i &lt; 3; i++){
            var t_piece = piece_grid[ i, puzzle_y];
            t_piece.x = dif_x + sprite_w * i;
            if(t_piece.x &gt; sprite_w * 3){
                t_piece.x -= sprite_w * 3;
            } else if(t_piece.x &lt; 0){
                t_piece.x += sprite_w * 3;
            }
            
        }
    } else {
        for(var i = 0; i &lt; 3; i++){
            var t_piece = piece_grid[ puzzle_x, i];
            t_piece.y = dif_y + sprite_w * i;
            if(t_piece.y &gt; sprite_w * 3){
                t_piece.y -= sprite_w * 3;
            } else if(t_piece.y &lt; 0){
                t_piece.y += sprite_w * 3;
            }
            
        }
    }
}
if(mouse_check_button_released(mb_left)){
    dif_x = mouse_x - start_x;
    dif_y = mouse_y - start_y;
    if( abs(dif_x) &gt; abs(dif_y) and abs(dif_x) &gt; sensitivity){
        step = dif_x div sprite_w;
        temp_a = piece_grid[ 0, puzzle_y];
        temp_b = piece_grid[ 1, puzzle_y];
        temp_c = piece_grid[ 2, puzzle_y];
        if(sign(step) == -1){
            piece_grid[ 0, puzzle_y] = temp_b
            piece_grid[ 1, puzzle_y] = temp_c
            piece_grid[ 2, puzzle_y] = temp_a
            
        } else if(sign(step) == 1){
            piece_grid[ 0, puzzle_y] = temp_c
            piece_grid[ 1, puzzle_y] = temp_a
            piece_grid[ 2, puzzle_y] = temp_b
            
        }
    } else if(abs(dif_y) &gt; sensitivity){
        step = dif_y div sprite_w;
        temp_a = piece_grid[ puzzle_x, 0];
        temp_b = piece_grid[ puzzle_x, 1];
        temp_c = piece_grid[ puzzle_x, 2];
        if(sign(step) == -1){
            piece_grid[ puzzle_x, 0] = temp_b
            piece_grid[ puzzle_x, 1] = temp_c
            piece_grid[ puzzle_x, 2] = temp_a
            
        } else if(sign(step) == 1){
            piece_grid[ puzzle_x, 0] = temp_c
            piece_grid[ puzzle_x, 1] = temp_a
            piece_grid[ puzzle_x, 2] = temp_b
            
        }
    }
}
